<div class="one-item-check">
    <span class="id_goods_item"><%= item.id_goods%></span>
    <div class="caption">
        <div class="counter">
            <%= item.counter%>
        </div>
        <div class="item-name">
            <%= item.model%>
        </div>
        <div class="price">
            <%= item.price%>$
        </div>
        <div class="item-quantity">
            <span class="label label-info counts"><%= item.quantity%></span>
        </div>
    </div>
</div>